{
    "name": "symfony/marshaller",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "phpstan/phpdoc-parser": "^1.13",
        "phpdocumentor/reflection-docblock": "^5.3",
        "psr/cache": "^3.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "symfony/cache": "^6.2"
    },
    "suggest": {
        "psr/cache-implementation": "To cache results"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Marshaller\\": "" },
        "files": [ "Resources/functions.php" ],
        "exclude-from-classmap": [ "Tests/" ]
    }
}
