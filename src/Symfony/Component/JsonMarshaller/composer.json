{
    "name": "symfony/json-marshaller",
    "type": "library",
    "description": "Provides powerful methods to marshal/unmarshal data structures into/from JSON.",
    "keywords": ["serialization", "marshalling"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Arlaud",
            "email": "mathias.arlaud@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/container": "^2.0|^3.0",
        "psr/log": "^1|^2|^3",
        "symfony/var-exporter": "^7.0"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^5.0",
        "phpstan/phpdoc-parser": "^1.0",
        "symfony/http-kernel": "^7.0"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\JsonMarshaller\\": "" },
        "exclude-from-classmap": [ "Tests/" ]
    },
    "minimum-stability": "dev"
}
